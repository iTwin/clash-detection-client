{
  "name": "@itwin/clash-detection-client",
  "version": "0.1.0",
  "description": "Clash Detection client for the iTwin platform",
  "main": "lib/cjs/clash-detection-client.js",
  "module": "lib/esm/clash-detection-client.js",
  "typings": "lib/cjs/clash-detection-client",
  "scripts": {
    "build": "npm run -s build:cjs && npm run -s build:esm",
    "build:cjs": "tsc 1>&2 --outDir lib/cjs",
    "build:esm": "tsc 1>&2 --module ES2020 --outDir lib/esm",
    "lint": "eslint -f visualstudio \"./src/**/*.ts\" 1>&2",
    "lint:fix": "npm run lint -- --fix",
    "webpackTests": "webpack --config src/test/utils/webpack.config.js 1>&2",
    "test": "npm run -s webpackTests && npm run -s test:chrome",
    "test:chrome": "certa -r chrome",
    "pack": "npm pack",
    "clean": "rimraf lib",
    "docs": "betools docs --tsIndexFile=clash-detection-client.ts --json=./docs/file.json --onlyJson"
  },
  "keywords": [
    "Bentley",
    "iTwin",
    "digital-twin",
    "clash-detection",
    "IModelHub"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/clash-detection-client"
  },
  "dependencies": {
    "@itwin/imodels-client-management": "^1.1.0",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@itwin/build-tools": "^3.2.7",
    "@itwin/certa": "^3.2.7",
    "@itwin/core-bentley": "^3.2.7",
    "@itwin/eslint-plugin": "^3.2.7",
    "@itwin/oidc-signin-tool": "^3.4.1",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.14.31",
    "chai": "^4.3.6",
    "cpx": "^1.5.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^7.32.0",
    "mocha": "^9.2.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.1.3",
    "typedoc": "^0.23.10",
    "typedoc-plugin-markdown": "^3.13.4",
    "typescript": "~4.6.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
    ],
    "extends": "plugin:@itwin/itwinjs-recommended",
    "rules": {
      "no-duplicate-imports": "off",
      "@typescript-eslint/consistent-type-imports": "error"
    }
  },
  "pnpm": {
    "overrides": {
      "glob-parent@<5.1.2": ">=5.1.2",
      "validator@<13.7.0": ">=13.7.0",
      "braces@<2.3.1": ">=2.3.1",
      "nanoid@>=3.0.0 <3.1.31": ">=3.1.31"
    }
  }
}